{
  "name": "n8n-nodes-azure-devops",
  "version": "0.1.1",
  "description": "Azure DevOps integration for n8n",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "n8n-integration",
    "azure",
    "azure-devops",
    "azure-pipelines",
    "azure-repos",
    "azure-boards",
    "azure-work-items",
    "azure-projects",
    "devops",
    "boards",
    "repos",
    "ci-cd",
    "git",
    "repository",
    "pipeline",
    "automation",
    "integration",
    "api",
    "microsoft",
    "microsoft-azure"
  ],
  "license": "MIT",
  "author": {
    "name": "Guilherme Tadeu",
    "email": "guilhermetadeu3635@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SheickBR/n8n-nodes-azure-devops.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AzureDevOpsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AzureDevOps/AzureDevOps.node.js",
      "dist/nodes/AzureDevOps/AzureDevOpsTrigger.node.js"
    ]
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}
